{
  "documentationUrl": "https://docsurl.com",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Google Directory Spec",
    "type": "object",
    "required": ["credentials_json", "credentials_pickle"],
    "additionalProperties": false,
    "properties": {
      "credentials_json": {
        "type": "string",
        "description": "Path to the JSON credentials file.",
        "airbyte_secret": true
      },
      "credentials_pickle": {
        "type": "string",
        "description": "Path to the pickle credentials file.",
        "airbyte_secret": true
      }
    }
  }
}
